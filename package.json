{
  "name": "divops-slides",
  "description": "Slidev deck about divops",
  "license": "MIT",
  "private": true,
  "repository": "travi/divops-slides",
  "bugs": "https://github.com/travi/divops-slides/issues",
  "homepage": "https://github.com/travi/divops-slides#readme",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:*",
    "lint:lockfile": "lockfile-lint",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "run-s lint:js -- --fix",
    "lint:md": "remark . --frail",
    "generate:md": "remark . --output",
    "lint:sensitive": "ban",
    "prepare": "husky install",
    "clean": "rimraf ./lib",
    "start": "node ./lib/index.js",
    "prebuild": "run-s clean",
    "dev": "slidev",
    "build": "slidev build",
    "export": "slidev export",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open",
    "test:served:vulnerable": "is-website-vulnerable http://localhost:3030",
    "test:served": "start-server-and-test 'npm run dev' http://localhost:3030/1 'npm-run-all --print-label --parallel test:served:*'",
    "test:served:smoke": "run-s cypress:run",
    "lint:peer": "npm ls >/dev/null"
  },
  "dependencies": {
    "@slidev/cli": "0.16.0",
    "@slidev/theme-default": "0.14.2",
    "vue-template-compiler": "2.6.12"
  },
  "devDependencies": {
    "@babel/register": "7.13.16",
    "@travi/any": "2.0.16",
    "@travi/babel-preset": "3.0.57",
    "@travi/eslint-config": "1.0.73",
    "@travi/eslint-config-cypress": "1.0.27",
    "ban-sensitive-files": "1.9.15",
    "commitlint-config-travi": "1.3.16",
    "cypress": "7.4.0",
    "cz-conventional-changelog": "3.3.0",
    "husky": "6.0.0",
    "is-website-vulnerable": "1.14.7",
    "lockfile-lint": "4.6.2",
    "npm-run-all": "4.1.5",
    "remark-cli": "9.0.0",
    "remark-preset-lint-travi": "1.3.13",
    "remark-toc": "7.2.0",
    "rimraf": "3.0.2",
    "start-server-and-test": "1.12.3"
  }
}
